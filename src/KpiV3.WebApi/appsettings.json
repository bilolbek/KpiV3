{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "AllowedHosts": "*",

  "ConnectionStrings": {
    "Default": "<CONNECTION_STRING>"
  },

  "PasswordGenerator": {
    "Length": 8,
    "IncludeDigits": true,
    "IncludeUppercase": false,
    "IncludeSymbols": false
  },

  "EmailSender": {
    "FromAddress": "<FROM_ADDRESS>",
    "FromAddressName": "<FROM_ADDRESS_NAME>",
    "SmtpServerAddress": "<SMTP_SERVER_ADDRESS>",
    "SmtpServerPort": 0,
    "SmtpServerSsl": false,
    "Password": "<PASSWORD>"
  }
}
